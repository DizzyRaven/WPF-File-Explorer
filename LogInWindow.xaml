<Window x:Class="CSharpProject.LogInWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSharpProject"
   
        mc:Ignorable="d"
        Title="LogInWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Username" Margin="305,81,408.6,305"/>
        <TextBox x:Name="username" Margin="261,120,359.6,267" />
        <Label Content="Password" Margin="305,214,408.6,171"/>
        <PasswordBox x:Name="password" PasswordChar="*" MinHeight="30" Margin="261,249,359.6,141" />

        <Button Content="LogIn" Click="Button_Click" Margin="224,314,341.6,68"/>

    </Grid>
</Window>
